CODE = gen3d

SRCS = \
arcxyz.f arcyxz.f comand.f dbmir1.f dbmirr.f exparc.f \
felcen.f fnpcen.f gen3d.f  getint.f getpro.f getspl.f \
getspt.f idblnk.f ixlink.f makcol.f makrow.f \
mirss.f  mrkcen.f newel1.f newess.f newid.f  newini.f \
newmap.f newnps.f newxyz.f proxyz.f rdelb.f  rdspln.f \
relblk.f renumb.f rotxyz.f show.f   shwint.f spline.f \
splxyz.f sptxyz.f trnxyz.f twixyz.f usblk.f  usids.f  \
wrelb.f  wress.f  wrnps.f  wrpxyz.f dbpini.f dboqa.f


OBJS = ${SRCS:.f=.o}

LIBS = $(EXOIIV2FOR) $(EXOIIV2C) $(NETCDF) $(SUPLIB) $(SUPES)

NormalFortranTarget($(CODE),$(OBJS),NullParameter,NullParameter,$(LIBS))

InstallProgram($(CODE),$(BINDIR))

DependTarget()

